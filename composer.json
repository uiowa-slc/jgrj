{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.2",
        "md/publication-project": "dev-ss4.x",
        "sunra/php-simple-html-dom-parser": "v1.5.0",
        "axllent/silverstripe-version-truncator": "^2.0"
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/publication-project"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/deploy-helper"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/silverstripe-maintenance-mode"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/silverstripe-staticpublishqueue"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/uiowa-bar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/ac-json-events"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/silverstripe-flickr"
        },
        {
            "type": "vcs",
            "url": "https://github.com/uiowa-slc/silverstripe-saml"
        }
    ],
    "extra": {
        "resources-dir": "_resources",
        "expose": [
            "themes/jgrj/dist",
            "themes/jgrj/css"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
